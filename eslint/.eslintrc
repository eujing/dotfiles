{
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true,
        "es6": true,
    },

    "settings": {
        "ecmascript": 6,
        "jsx": true
    },

    "ecmaFeatures": {
        "jsx": true
    },

    "plugins": [
        "react"
    ],

    "rules": {
        "strict": 0,
        "semi": [2, "always"],
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0
    }
}
